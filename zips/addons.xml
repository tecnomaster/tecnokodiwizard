<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.tecnokodiwizard" name="[B][COLOR dodgerblue]TecnoKodi[/COLOR][/B] Wizard" version="1.1" provider-name="TecnoKodi">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[B][COLOR dodgerblue]TecnoKodi[/COLOR][/B] Wizard</summary>
    <description lang="en">[B][COLOR dodgerblue]TecnoKodi[/COLOR][/B] Wizard ofrece muchas funciones de mantenimiento, guardado selectivo de datos adicionales e instalaciones automatizadas.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.tecnokodiwizard" name="TecnoKodi Wizard official Repo" version="1.0" provider-name="TecnoKodi">
    <extension point="xbmc.addon.repository" name="TecnoKodi Wizard official Repo">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/tecnomaster/tecnokodiwizard/master/zips/addon.xml</info>
            <checksum>https://raw.githubusercontent.com/tecnomaster/tecnokodiwizard/master/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/tecnomaster/tecnokodiwizard/master/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>TecnoKodi Wizard official Repo</summary>
        <description>Repositorio Oficial de TecnoKodi Wizard</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>
</addons>
